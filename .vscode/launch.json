{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Unit Test Private Fn",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/MyClassTest",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "Check setup script environment"
        }
    ]
}